	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'ota-dump'",
			"cargo": {
				"args": [
					"build",
					"--bin=ota-dump",
					"--package=ota-dump"
				],
				"filter": {
					"name": "ota-dump",
					"kind": "bin"
				}
			},
			"args": [
				"/usr/local/google/home/zhangkelvin/aosp/cf_x86_keystore2.zip"
			],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'ota-dump'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=ota-dump",
					"--package=ota-dump"
				],
				"filter": {
					"name": "ota-dump",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
